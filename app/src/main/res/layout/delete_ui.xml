<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardMaxElevation="0dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="0dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="20dp">

    
        <TextView
            android:id="@+id/id_delete_text_2"
            android:textStyle="bold"
            android:textColor="@color/color_515151"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:text="@string/mood_mood_list"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:textSize="15sp"/>
        
        
        <TextView
            android:id="@+id/id_delete_text_3"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:textColor="@color/color_ffffff"
            android:background="@drawable/delete_buttom_bg"
            android:text="@string/mood_complete"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="13sp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"/>
        
        <TextView
            android:id="@+id/id_delete_text_4"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:textColor="@color/color_515151"
            android:background="@drawable/delete_buttom_bg_1"
            android:text="@string/mood_complete"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="13sp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"/>
        
        
        

    </LinearLayout>

</androidx.cardview.widget.CardView>

